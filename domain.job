#!/bin/bash
#SBATCH -N 20
#SBATCH -p RM
#SBATCH --ntasks-per-node 1
#SBATCH -t 8:00:00
#SBATCH --job-name=Sbicolor-v3.1.1-domain
#SBATCH --mail-type=ALL
#SBATCH --mail-user=you@domain.com
#SBATCH -o %x.out
#SBATCH -e %x.err


source load_modules.sh
export GOMAP_LOC="/path/to/GOMAP-singularity/"
export tmpdir="$LOCAL"

$GOMAP_LOC/run-GOMAP-mpi.sh --step=domain --config=config.yaml
